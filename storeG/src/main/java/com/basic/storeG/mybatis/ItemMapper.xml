<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.item">
	<resultMap type="com.basic.storeG.vo.Item" id="item">
		<id column="item_no" property="itemNo"/>
	    <result column="qty" property="quantity"/>
	    <result column="img" property="image"/>
	    <result column="pName" property="productName"/>
	    <result column="price" property="price"/>
	    <result column="category" property="category"/>
	    <result column="rating" property="rating"/>
	    <result column="data" property="data"/>
	    <result column="register_date" property="registerDate"/>
	</resultMap>
	
	
	
</mapper>

